    "import os\n",
    "files = os.listdir()\n",
    "new_name = \"-thumb\"\n",
    "sno = 1\n",
    "extension = \".jpg\"\n",
    "for file in files:\n",
    "    if(file.rfind('jpg')>0):\n",
    "        nn = str(sno)+new_name+extension\n",
    "        os.rename(file,nn)\n",
    "        sno = sno + 1\n",
    "print(files)"
